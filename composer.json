{
    "name": "bigwebbro/sdk",
    "description": "SDK for integration with payment system",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Tiyn\\MerchantApiSdk\\": "src/"
        }
    },
    "authors": [
        {
            "name": "bigwebbro",
            "email": "bigwebbro@yandex.ru"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": ">=7.4.5",
        "psr/http-client": "^1.0",
        "psr/http-message": "^2.0",
        "symfony/validator": ">=v6.4.11",
        "symfony/serializer": ">=v6.1.0",
        "symfony/property-access": ">=v6.1.0",
        "symfony/property-info": "^6.4",
        "phpdocumentor/reflection-docblock": "^5.6",
        "jms/serializer": "^3.32"
    },
    "require-dev": {
        "phpstan/phpstan": "2.1.31",
        "rector/rector": "2.2.5",
        "friendsofphp/php-cs-fixer": "3.89.1",
        "phpunit/phpunit": "10.5.58",
